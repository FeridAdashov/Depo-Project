            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Pages.DEPO.Fragments.Debts.ShowDebt.ShowDebtActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Pages.SELLER.Activities.Basket.SellerBasketActivity"
