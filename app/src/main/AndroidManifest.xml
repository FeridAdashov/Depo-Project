            android:name=".Pages.RegisterActivities.DepoRegisterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Pages.RegisterActivities.BuyerRegisterActivity"
