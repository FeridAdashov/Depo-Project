        android:icon="@mipmap/logo_depo"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/logo_depo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
